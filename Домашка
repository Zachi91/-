byte тоннЗерна = 255; // от 0 до 255
string = "Привет, Мир"; // сообщение в строку
char валюта = '$'; // одиночный символ
bool этоДомашка = true; // 2 показателя - ложь = false | правда = true
flout деньги = 1234567; // 7 знаков
sbyte деньги = -120; // -128 до 127
short бюджет = -32000; // от -32768 до 32767
int картошка = 20; // от -2147483648 до 2147483647
ushort овощи = 30; // 0 до 65535
uint петрушка = 25; // 0 до 4294367295
